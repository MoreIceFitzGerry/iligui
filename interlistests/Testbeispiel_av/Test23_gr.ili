CONTRACTED MODEL Test23_gr (de) AT "http://www.interlis.ch/" VERSION "2015-03-06" =

  IMPORTS Test23;

  UNIT
    Grads = PI / 200.0 [INTERLIS.rad];

  DOMAIN
    Point (ABSTRACT) = COORD NUMERIC,NUMERIC;
    Surface (ABSTRACT) = SURFACE WITH (STRAIGHTS, ARCS) VERTEX Point;

  FUNCTION Inside(p:Point; s:Surface): BOOLEAN
      //liefert True wenn p innerhalb s liegt//;

  DOMAIN
    Schriftgroesse = (
      klein,
      mittel,
      gross);
    Schriftstil = (
      normal,
      gesperrt,
      weitere);
    Rotation = 0.0 .. 399.9 [Grads];

    STRUCTURE TextPos =
      ATTRIBUTE
        Pos: MANDATORY Test23.LKoord;
        Ori: Rotation;
        HAli: HALIGNMENT;
        VAli: VALIGNMENT;
      MANDATORY CONSTRAINT
        DEFINED(HAli) == DEFINED(VAli);
      MANDATORY CONSTRAINT
        DEFINED(VAli) == DEFINED(HAli);
    END TextPos;

    STRUCTURE TextPosGroesse EXTENDS TextPos =
      ATTRIBUTE
        Groesse: Schriftgroesse;
    END TextPosGroesse;

    STRUCTURE TextPosGroesseStil EXTENDS TextPosGroesse =
      ATTRIBUTE
        Stil: Schriftstil;
    END TextPosGroesseStil;


