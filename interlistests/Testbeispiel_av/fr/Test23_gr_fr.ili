CONTRACTED MODEL Test23_gr_fr (fr) AT "http://www.interlis.ch/" VERSION "2015-03-06"
                                   TRANSLATION OF Test23_gr ["2015-03-06"] =
  IMPORTS Test23_fr;

  UNIT
    Grads = PI / 200.0 [INTERLIS.rad];

  DOMAIN
    Point (ABSTRACT) = COORD NUMERIC,NUMERIC;
    Surface (ABSTRACT) = SURFACE WITH (STRAIGHTS, ARCS) VERTEX Point;

  FUNCTION Inside(p:Point; s:Surface):BOOLEAN
      //fournit True si p est situé dans s//;

  DOMAIN
    GrandeurEcriture = (
      petite,
      moyenne,
      grande);
    StyleEcriture = (
      normal,
      ecarte,
      autre);
    Rotation = 0.0 .. 399.9 [Grads];

    STRUCTURE PosTexte =
      ATTRIBUTE
        Pos: MANDATORY Test23_fr.CoordP;
        Ori: Rotation;
        HAli: HALIGNMENT;
        VAli: VALIGNMENT;
      MANDATORY CONSTRAINT
        DEFINED(HAli) == DEFINED(VAli);
      MANDATORY CONSTRAINT
        DEFINED(VAli) == DEFINED(HAli);
    END PosTexte;

    STRUCTURE GrandeurPosTexte EXTENDS PosTexte =
      ATTRIBUTE
        Grandeur: GrandeurEcriture;
    END GrandeurPosTexte;

    STRUCTURE StyleGrandeurPosTexte EXTENDS GrandeurPosTexte =
      ATTRIBUTE
        Style: StyleEcriture;
    END StyleGrandeurPosTexte;


