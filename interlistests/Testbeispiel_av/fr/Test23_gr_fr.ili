  TOPIC Bords_de_plan =

    OID AS INTERLIS.STANDARDOID;
    DEPENDS ON Test23_fr.Domaines_numerotation;

    DOMAIN
      Type_echelle = 1 .. 1000000;
      Genre_description = (
        voisins,
        plan_voisin,
        plan_synoptique,
        direction_route,
        no_CN,
        NoOFS,
        autre);
      Type_ligne = (
        autre);
      Genre_symbole = (
        flecheNord,
        autre);
      Genre_croix = (
        croix_coord,
        Croix_filet,
        marque_filet,
        autre);

    CLASS Bord_de_plan =
      ATTRIBUTE
        IdentDN: MANDATORY TEXT*8;
        Identification: MANDATORY TEXT*32;
        Type_bord_de_plan: MANDATORY TEXT*20;
        Numero_du_plan: MANDATORY TEXT*12;
        Nom_commune: MANDATORY TEXT*30;
        Nom_geometre: TEXT*30;
        Date_etablissement: MANDATORY Test23_fr.Date;
        Nom_geometre_conservateur: TEXT*30;
        Date_MAJ: Test23_fr.Date;
        Nombre_echelle: MANDATORY Type_echelle;
        Origine_plan: MANDATORY Test23_fr.CoordP;
        E_Azimut: MANDATORY Rotation;
        Nombre_echelle_plan_synoptique: Type_echelle;
        Origine_plan_synoptique: Test23_fr.CoordP;
        Avec_reseau_coord: MANDATORY (
          oui,
          non);
      UNIQUE
        IdentDN, Identification;
      EXISTENCE CONSTRAINT
        IdentDN REQUIRED IN Test23_fr.Domaines_numerotation.Domaine_numerotation:NumeroDN;
    END Bord_de_plan;

    CLASS Description_plan =
      ATTRIBUTE
        Description: MANDATORY TEXT*30;
        Genre: MANDATORY Genre_description;
    END Description_plan;

    ASSOCIATION Description_planAssoc =
       Objet -- {1} Bord_de_plan;
       Description_plan -- {*} Description_plan;
    END Description_planAssoc;

    CLASS PosDescription_plan =
      ATTRIBUTE
        Pos: MANDATORY GrandeurPosTexte;
    END PosDescription_plan;

