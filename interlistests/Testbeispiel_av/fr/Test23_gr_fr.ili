       Indication_coordonnees -- {*} Indication_coordonnees;
    END Indication_coordonneesAssoc;

    CLASS PosIndication_coordonnees =
      ATTRIBUTE
        Pos: MANDATORY GrandeurPosTexte;
      UNIQUE Pos;
    END PosIndication_coordonnees;

    ASSOCIATION PosIndication_coordonneesAssoc =
       Objet -- {1} Indication_coordonnees;
       PosIndication_coordonnees -- {*} PosIndication_coordonnees;
