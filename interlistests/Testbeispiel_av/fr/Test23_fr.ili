!!
!! Modèle de base pour les données test INTERLIS 2.3. Le modèle de base
!! est dérivé du modèle INTERLIS 1 MD01MOCH23F.
!! Test23_fr n'est par contre pas une traduction officielle du MD01MOCH23F
!! vers INTERLIS 2.3. Le modèle de données Test23_fr a uniquement pour
!! but de montrer les concepts d'INTERLIS 2.3 par un exemple connu.
!!
!!
!! Version: 1.0
!! Langue: français
!! Nom du fichier: Test23_fr.ili
!!
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

MODEL Test23_fr (fr) AT "http://www.interlis.ch/" VERSION "2015-03-06"
                     TRANSLATION OF Test23 ["2015-03-06"] =

  IMPORTS Units;
  IMPORTS Time;

  DOMAIN
    Date = FORMAT INTERLIS.XMLDate "1582-10-15" .. "2999-12-31";

    CoordP = COORD 480000.000 .. 840000.000 [INTERLIS.m],
                   70000.000 .. 300000.000 [INTERLIS.m],
                   ROTATION 2 -> 1;
    CoordA = COORD 480000.000 .. 840000.000 [INTERLIS.m],
                   70000.000 .. 300000.000 [INTERLIS.m],
                   -200.000 .. 5000.000 [INTERLIS.m],
                   ROTATION 2 -> 1;

!!  Altitude = -200.000 .. 5000.000 [INTERLIS.m] {SwissOrthometricAlt};
    Altitude = -200.000 .. 5000.000 [INTERLIS.m];

    Precision = 0.0 .. 700.0;

    Fiabilite = (
      oui,
      non);

    Statut = (
      projete,
      valable);

    StandardQualite = (
      MO93,
      MP74,
      NP,
      autre);

    Materiel = (
      borne,
      borne_artificielle,
      cheville,
      tuyau,
      pieu,
      croix,
      non_materialise);


  TOPIC Domaines_numerotation =
    DOMAIN
      Abreviation_cantonale = (
        ZH,
        BE,
        LU,
        UR,
        SZ,
        OW,
        NW,
        GL,
        ZG,
        FR,
        SO,
        BS,
        BL,
        SH,
        AR,
        AI,
        SG,
        GR,
        AG,
        TG,
        TI,
        VD,
        VS,
        NE,
        GE,
        JU,
        FL,
        CH);

    CLASS Domaine_numerotation =
      ATTRIBUTE
        Ct: MANDATORY Abreviation_cantonale;
        NumeroDN: MANDATORY TEXT*6;
        DossierTech: MANDATORY TEXT*12;
        Date: Date;
      UNIQUE
        Ct, NumeroDN;
    END Domaine_numerotation;

    CLASS GeometrieDN =
      ATTRIBUTE
        Geometrie: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX CoordP
          WITHOUT OVERLAPS > 0.050;
    END GeometrieDN;

    ASSOCIATION GeometrieDNAssoc =
      Objet -- {1} Domaine_numerotation;
      GeometrieDN -- {*} GeometrieDN;
    END GeometrieDNAssoc;

  END Domaines_numerotation;


  TOPIC Points_fixesCategorie1 =

    CLASS PFP1 =
      ATTRIBUTE
        Numero: MANDATORY TEXT*12;
        Geometrie: MANDATORY CoordP;
        GeomAlt: Altitude;
        PrecPlan: MANDATORY Precision;
        FiabPlan: MANDATORY Fiabilite;
        PrecAlt: Precision;
        FiabAlt: Fiabilite;
        Accessibilite: MANDATORY (
          accessible,
          inaccessible);
      UNIQUE
        Numero;
      UNIQUE
        Geometrie, GeomAlt;
      MANDATORY CONSTRAINT
        DEFINED(PrecAlt) == DEFINED(GeomAlt);
      MANDATORY CONSTRAINT
        DEFINED(FiabAlt) == DEFINED(GeomAlt);
    END PFP1;

    CLASS PFA1 =
      ATTRIBUTE
        Numero: MANDATORY TEXT*12;
        Geometrie: MANDATORY CoordP;
        GeomAlt: MANDATORY Altitude;
        PrecPlan: Precision;
        FiabPlan: Fiabilite;
        PrecAlt: MANDATORY Precision;
        FiabAlt: MANDATORY Fiabilite;
      UNIQUE
        Numero;
      UNIQUE
        Geometrie;
    END PFA1;

  END Points_fixesCategorie1;


  TOPIC Points_fixesCategorie2 =
    DEPENDS ON Test23_fr.Domaines_numerotation;

    CLASS PFP2 =
      ATTRIBUTE
        IdentDN: MANDATORY TEXT*8;
        Numero: MANDATORY TEXT*12;
        Geometrie: MANDATORY CoordP;
        GeomAlt: Altitude;
        PrecPlan: MANDATORY Precision;
        FiabPlan: MANDATORY Fiabilite;
        PrecAlt: Precision;
        FiabAlt: Fiabilite;
        Accessibilite: MANDATORY (
          accessible,
          inaccessible);
      UNIQUE
        IdentDN, Numero;
      UNIQUE
        Geometrie, GeomAlt;
      MANDATORY CONSTRAINT
        DEFINED(PrecAlt) == DEFINED(GeomAlt);
      MANDATORY CONSTRAINT
        DEFINED(FiabAlt) == DEFINED(GeomAlt);
      EXISTENCE CONSTRAINT
        IdentDN REQUIRED IN Test23_fr.Domaines_numerotation.Domaine_numerotation:NumeroDN;
    END PFP2;

    CLASS PFA2 =
      ATTRIBUTE
        IdentDN: MANDATORY TEXT*8;
        Numero: MANDATORY TEXT*12;
        Geometrie: MANDATORY CoordP;
        GeomAlt: MANDATORY Altitude;
        PrecPlan: Precision;
        FiabPlan: Fiabilite;
        PrecAlt: MANDATORY Precision;
        FiabAlt: MANDATORY Fiabilite;
      UNIQUE
        IdentDN, Numero;
      UNIQUE
        Geometrie;
      EXISTENCE CONSTRAINT
        IdentDN REQUIRED IN Test23_fr.Domaines_numerotation.Domaine_numerotation:NumeroDN;
    END PFA2;

  END Points_fixesCategorie2;


  TOPIC Points_fixesCategorie3 =
    DEPENDS ON Test23_fr.Domaines_numerotation;

    CLASS PFP3 =
      ATTRIBUTE
        IdentDN: MANDATORY TEXT*8;
        Numero: MANDATORY TEXT*12;
        Geometrie: MANDATORY CoordP;
        GeomAlt: Altitude;
        PrecPlan: MANDATORY Precision;
        FiabPlan: MANDATORY Fiabilite;
        PrecAlt: Precision;
        FiabAlt: Fiabilite;
        Signe: MANDATORY Materiel;
        Fiche: MANDATORY BOOLEAN;
      UNIQUE
        IdentDN, Numero;
      UNIQUE
        Geometrie;
      MANDATORY CONSTRAINT
        Signe != #non_materialise;
      MANDATORY CONSTRAINT
        DEFINED(PrecAlt) == DEFINED(GeomAlt);
      MANDATORY CONSTRAINT
        DEFINED(FiabAlt) == DEFINED(GeomAlt);
      EXISTENCE CONSTRAINT
        IdentDN REQUIRED IN Test23_fr.Domaines_numerotation.Domaine_numerotation:NumeroDN;
    END PFP3;

    CLASS PFA3 =
      ATTRIBUTE
        IdentDN: MANDATORY TEXT*8;
        Numero: MANDATORY TEXT*12;
        Geometrie: MANDATORY CoordP;
        GeomAlt: MANDATORY Altitude;
        PrecPlan: Precision;
        FiabPlan: Fiabilite;
        PrecAlt: MANDATORY Precision;
        FiabAlt: MANDATORY Fiabilite;
      UNIQUE
        IdentDN, Numero;
      UNIQUE
        Geometrie;
      EXISTENCE CONSTRAINT
        IdentDN REQUIRED IN Test23_fr.Domaines_numerotation.Domaine_numerotation:NumeroDN;
    END PFA3;

  END Points_fixesCategorie3;


  TOPIC Couverture_du_sol =

    DOMAIN
      Genre_CS = (
        batiment,
        revetement_dur (
          route_chemin,
          trottoir,
          ilot,
          chemin_de_fer,
          place_aviation,
          bassin,
          autre_revetement_dur),
        verte (
          champ_pre_paturage,
          culture_intensive (
            vigne,
            autre_culture_intensive),
          jardin,
          tourbiere,
          autre_verte),
        eau (
          eau_stagnante,
          cours_eau,
          roseliere),
        boisee (
          foret_dense,
          autre_boisee),
        sans_vegetation (
          rocher,
          glacier_neve,
          eboulis_sable,
          graviere_decharge,
          autre_sans_vegetation));

    CLASS ObjetCouv (ABSTRACT) =
    END ObjetCouv;

    CLASS SurfaceCSProj EXTENDS ObjetCouv =
      ATTRIBUTE
        Qualite: MANDATORY StandardQualite;
        Genre: MANDATORY Genre_CS;
        Geometrie: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX CoordP
          WITHOUT OVERLAPS > 0.050;
    END SurfaceCSProj;

    CLASS NumeroBatimentProj =
      ATTRIBUTE
        Numero: MANDATORY TEXT*12;
    END NumeroBatimentProj;

    ASSOCIATION NumeroBatimentProjAssoc =
      Objet -- {1} SurfaceCSProj;
      NumeroBatimentProj -- {*} NumeroBatimentProj;
      MANDATORY CONSTRAINT
         Objet->Genre == #batiment;
    END NumeroBatimentProjAssoc;

    CLASS NomObjetProj =
      ATTRIBUTE
        Nom: MANDATORY TEXT*30;
    END NomObjetProj;

    ASSOCIATION NomObjetProjAssoc =
      Objet -- {1} SurfaceCSProj;
      NomObjetProj -- {*} NomObjetProj;
      MANDATORY CONSTRAINT
        Objet->Genre != #batiment;
    END NomObjetProjAssoc;

    CLASS SurfaceCS EXTENDS ObjetCouv =
      ATTRIBUTE
        Geometrie: MANDATORY AREA WITH (STRAIGHTS, ARCS) VERTEX CoordP
          WITHOUT OVERLAPS > 0.050;
        Qualite: MANDATORY StandardQualite;
        Genre: MANDATORY Genre_CS;
    END SurfaceCS;

    CLASS Numero_de_batiment =
      ATTRIBUTE
        Numero: MANDATORY TEXT*12;
    END Numero_de_batiment;

    ASSOCIATION Numero_de_batimentAssoc =
      Objet -- {1} SurfaceCS;
      Numero_de_batiment -- {*} Numero_de_batiment;
      MANDATORY CONSTRAINT
        Objet->Genre == #batiment;
    END Numero_de_batimentAssoc;

    CLASS Nom_objet =
      ATTRIBUTE
        Nom: MANDATORY TEXT*30;
    END Nom_objet;

    ASSOCIATION Nom_objetAssoc =
      Objet -- {1} SurfaceCS;
      Nom_objet -- {*} Nom_objet;
      MANDATORY CONSTRAINT
        Objet->Genre != #batiment;
    END Nom_objetAssoc;

    CLASS Point_particulier EXTENDS ObjetCouv =
      ATTRIBUTE
        Identification: TEXT*12;
        Geometrie: MANDATORY CoordP;
        PrecPlan: MANDATORY Precision;
        FiabPlan: MANDATORY Fiabilite;
        Defini_exactement: MANDATORY BOOLEAN;
      UNIQUE
        Geometrie;
    END Point_particulier;

  END Couverture_du_sol;


